
#--------------------#
# Personal Variables #
#--------------------#


#--------------#
#    Path      #
#--------------#

case $1 in
    'icmbpriv20')
	export PATH=${PATH}:~/Store/bin:/opt/bin
	;;
    'pit3-linux')
	export PATH=~/Store/bin:/opt/bin:${PATH}
	;;
    'aries')
	;;
    *)
	echo 'Computer Name not found!!'
	echo 'Load default environment'
	;;
esac



#--------------#
#   Editor     #
#--------------#
  export EDITOR=/usr/bin/nano

  

